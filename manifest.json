{
  "name": "Kattis Hint Giver v2",
  "version": "2.0.0",
  "description": "Adds hints to problems on Kattis. Hint source: http://cpbook.net/methodstosolve",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://cpbook.net/"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.kattis.com/problems*"
      ],
      "exclude_matches": [
        "https://*.kattis.com/problems/*/statistics",
        "https://*.kattis.com/problems/*/submit"
      ],
      "run_at": "document_end",
      "js": [
        "jquery.min.js",
        "tooltip.js",
        "data.js",
        "addHint.js"
      ]
    }
  ],
  "content_security_policy": {},
  "manifest_version": 3
}